{
  "extends": "@sindresorhus/tsconfig",
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "outDir": "build",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowJs": true,
    "noImplicitAny": false
  },
  "include": [
    "src"
  ]
}